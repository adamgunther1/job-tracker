All Jobs for <strong><%= @company.name %></strong>

<% @jobs.each do |job| %>
  <div class="job_<%= job.id %>">
    <ul><%= job.title %> at <%= job.company.name %></ul>
    <%= link_to "Update Job", edit_company_job_path(job.company, job) %>
    <%= link_to "Delete", company_job_path(job.company, job), method: "delete" %>
  </div>
<% end %>
<br><br>
<%= link_to "Create New Job", new_company_job_path %>
